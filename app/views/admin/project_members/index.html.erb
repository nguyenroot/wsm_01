<div class="content">
  <div class="women_main">
    <div class="grids">
      <div class="panel panel-widget forms-panel">
        <div class="forms">
          <span>
            <%= link_to t(".new_project"), new_admin_project_member_path,
              class:"btn btn-default" %>
          </span>
        <div class="form-grids widget-shadow" data-example-id="basic-forms">
          <div class="form-title">
            <h4><%= t ".show" %></h4>
          </div>
          <div class="form-body">
            <table id="table_list" class="table table-striped table-bordered">
              <thead class="thead-inverse">
                <tr>
                  <th class="text-center"><%= t ".id_user" %></th>
                  <th class="text-center"><%= t ".id_project" %></th>
                  <th class="text-center"><%= t ".project_role" %></th>
                  <th class="text-center" colspan="2"><%= t ".actions" %>
                  </th>
                </tr>
              </thead>
              <tbody class="subject-index">
              <%= render "project_member" %>
              </tbody>
            </table>
            <%= paginate @project_members %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
